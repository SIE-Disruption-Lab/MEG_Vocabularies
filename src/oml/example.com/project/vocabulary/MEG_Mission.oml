vocabulary <http://example.com/project/vocabulary/MEG_Mission#> as meg_mission {
	extends <http://uaontologies.com/UA_Foundation/UA_Foundation#> as foundation
    extends <http://uaontologies.com/UA_Core/UA_Information#> as info
    extends <http://uaontologies.com/UA_Domain/UA_SystemArchitecture#> as sys_arch
	extends <http://uaontologies.com/UA_Domain/UA_Requirements#> as req
	
	// Aspects
	
	aspect OperationalThing
	
	aspect PhysicalThing
	
	aspect OperationalContext
	
	aspect ConfigurationContext

    
    // Concepts
    concept Mission < StrategicPhase[
//    	restricts some foundation:contains to ActualMissionPhase	
//    	restricts some foundation:contains to MissionScenario
    ]
	
	concept ActualMissionPhase < foundation:Process[
		restricts all foundation:isContainedIn to Mission
//    	restricts some foundation:isContainedIn to Mission
    ]
    
    concept StrategicPhase < foundation:Process[
    ]
    	
    concept MissionEngineeringThread < foundation:Function, PhysicalThing[
    	restricts some foundation:specificallyDependentOn to foundation:IndependentContinuant
    	restricts all implements to MissionThread
//	    restricts some implements to MissionThread
	    restricts all foundation:contains to MissionFunction
    	restricts some foundation:contains to MissionFunction
//    	restricts all defines to ActualMissionPhase
//    	restricts some defines to ActualMissionPhase
    ]
    
    concept MissionThread < OperationalActivity[
    	restricts all isImplementedBy to MissionEngineeringThread
//    	restricts some isImplementedBy to MissionEngineeringThread
    	restricts all foundation:contains to MissionTask
    	restricts some foundation:contains to MissionTask
    	restricts all req:satisfies to EnterpriseGoal
//    	restricts some req:satisfies to EnterpriseGoal
    	
    ]
    
    concept OperationalActivity < foundation:Process, OperationalThing[
    	
    ]
    
    concept MissionFunction < foundation:Function, PhysicalThing[
    	restricts some foundation:specificallyDependentOn to foundation:IndependentContinuant
    	restricts all implements to MissionTask
//	    restricts some implements to MissionTask
		restricts all foundation:isContainedIn to MissionEngineeringThread
    	restricts some foundation:isContainedIn to MissionEngineeringThread
    ]
    
    concept MissionTask < OperationalActivity[
    	restricts all isImplementedBy to MissionFunction
//    	restricts some isImplementedBy to MissionFunction
		restricts all foundation:isContainedIn to MissionThread
    	restricts some foundation:isContainedIn to MissionThread
    	restricts all req:satisfies to EnterpriseObjective
//    	restricts some req:satisfies to EnterpriseObjective
    ]
    
    
    concept OperationalArchitecture < sys_arch:System, OperationalThing, OperationalContext[
    	//restricts all isImplementedBy to ResourceArchitecture
    	restricts all foundation:contains to OperationalPerformer
    	restricts all implements to ActualMissionPhase
    ]
    
    concept ResourceArchitecture < sys_arch:System, PhysicalThing, ConfigurationContext[
//    	restricts all implements to OperationalArchitecture
		restricts all implements to ActualMissionPhase
		restricts all foundation:contains to CapabilityConfiguration
    ]
    
    concept CapabilityConfiguration < sys_arch:System, PhysicalThing, ConfigurationContext[
//    	restricts all implements to OperationalPerformer
		restricts all foundation:contains to CapabilityConfiguration
		restricts all foundation:isContainedIn to ConfigurationContext
    ]
    
    concept MissionScenario < foundation:Process[
    	restricts all foundation:contains to MissionVignette
//    	restricts some foundation:contains to MissionVignette	
    	restricts all foundation:isContainedIn to Mission
//    	restricts some foundation:isContainedIn to Mission
    ]
	
	concept MissionVignette < foundation:Process[
    	restricts all foundation:isContainedIn to MissionScenario
    	restricts some foundation:isContainedIn to MissionScenario	
    ]
    
    concept EnterpriseGoal < req:Requirement[
    	restricts all req:derivesRequirement to EnterpriseObjective
//    	restricts some info:isAbout to Mission
    	restricts some info:prescribes to Mission
    ]
    
    concept EnterpriseObjective < req:Requirement[
    	restricts all req:requirementDerivedFrom to EnterpriseGoal
//    	restricts some info:isAbout to ActualMissionPhase
    	restricts some info:prescribes to ActualMissionPhase
    ]
    
//    concept ForceComponent < sys_arch:System, OperationalThing[
//    	restricts all foundation:contains to OperationalPerformer
//    	restricts all foundation:isContainedIn to OperationalArchitecture
//    	restricts all isImplementedBy to CapabilityConfiguration
//    ]
    
    concept OperationalPerformer < sys_arch:System, OperationalThing, OperationalContext[
//    	restricts all foundation:isContainedIn to ForceComponent
		restricts all foundation:contains to OperationalPerformer
		restricts all foundation:isContainedIn to OperationalContext
    ]    
    
    concept ScenarioCondition < info:DescriptiveInformationEntity[
    	restricts all info:describes to MissionScenario
    	restricts some info:describes to MissionScenario
    ]
    
    concept VignetteCondition < info:DescriptiveInformationEntity[
    	restricts all info:describes to MissionVignette
    	restricts some info:describes to MissionVignette
    ]
    
    concept ActualOutcome < foundation:Quality[
    	restricts some req:satisfies to EnterpriseGoal
    	restricts some req:satisfies to EnterpriseObjective 
    ]
    
    concept OutcomeDescriptor < info:DescriptiveInformationEntity[
    	restricts all info:describes to ActualOutcome
    	restricts some info:describes to ActualOutcome
    	restricts all informs to Stakeholder
    	restricts some informs to Stakeholder
    ]
    
    concept Stakeholder < foundation:Role
    
    //Relation
	
	relation entity Defines [
		from MissionEngineeringThread
		to ActualMissionPhase
		forward defines
		reverse isDefinedBy
		asymmetric
		irreflexive
	]
	
	relation entity Implements [
		from foundation:IdentifiedEntity
		to foundation:IdentifiedEntity
		forward implements
		reverse isImplementedBy
		asymmetric
		irreflexive
	]
	
	relation entity Informs [
		from OutcomeDescriptor
		to Stakeholder
		forward informs
		reverse isInformedBy
		asymmetric
		irreflexive
	]
    
}